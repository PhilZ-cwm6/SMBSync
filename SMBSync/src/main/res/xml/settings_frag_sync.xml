<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android">
   	    <ListPreference
			android:key="@string/settings_network_wifi_option"
			android:title="@string/settings_network_wifi_option_title"
			android:summary="@string/settings_network_wifi_option_summary_connected_any_ap"
			android:dialogTitle="@string/settings_network_wifi_option_dialog_title"
			android:entries="@array/settings_network_wifi_option_list_entries"
			android:entryValues="@array/settings_network_wifi_option_list_values"/>

	    <CheckBoxPreference
			android:key="@string/settings_auto_start"
			android:title="@string/settings_auto_start_title"
			android:summary="@string/settings_auto_start_summary_ena" />
	    
	    <CheckBoxPreference
			android:key="@string/settings_auto_term"
			android:title="@string/settings_auto_term_title"
			android:summary="@string/settings_auto_term_summary_ena" 
			android:dependency="settings_auto_start"/>
	
	    <CheckBoxPreference
			android:key="@string/settings_backgroound_execution"
			android:title="@string/settings_backgroound_execution_title"
			android:summary="@string/settings_backgroound_execution_summary_ena" 
			android:dependency="settings_auto_start"/>
	  	    	    	    
	    <CheckBoxPreference
			android:key="@string/settings_error_option"
			android:title="@string/settings_error_option_title"
			android:summary="@string/settings_error_option_summary_ena" />

   	    <ListPreference
			android:key="@string/settings_keep_screen_on"
			android:title="@string/settings_keep_screen_on_title"
			android:dialogTitle="@string/settings_keep_screen_on_dialog_title"
			android:entries="@array/settings_keep_screen_on_list_entries"
			android:entryValues="@array/settings_keep_screen_on_list_values"/>
	    	    
	    <CheckBoxPreference
			android:key="@string/settings_wifi_lock"
			android:title="@string/settings_wifi_lock_title"
			android:summary="@string/settings_wifi_lock_summary_ena" />

	    <CheckBoxPreference
			android:key="@string/settings_remote_file_copy_by_rename"
			android:title="@string/settings_remote_file_copy_by_rename_title"
			android:summary="@string/settings_remote_file_copy_by_rename_summary_ena" />

	    <CheckBoxPreference
			android:key="@string/settings_local_file_copy_by_rename"
			android:title="@string/settings_local_file_copy_by_rename_title"
			android:summary="@string/settings_local_file_copy_by_rename_summary_ena" />
	    	    	    	    	    
</PreferenceScreen>